[flake8]
max-line-length = 120
exclude =
    .git,
    __pycache__,
    venv,
    .venv,
    *.egg-info,
    .tox,
    .pytest_cache,
    migrations,
    build,
    dist
extend-ignore = E203, W503
# E203: whitespace before ':' (conflicts with black)
# W503: line break before binary operator (conflicts with black)
